{
  "include": [
    "/**/*",
    "../packages/**/*",
    "../projects/**/*"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "esModuleInterop":true,
    "paths": {
      "@pkg/*": [
        "../packages/*"
      ],
      "@project-center/*": [
        "/*"
      ]
    }
  }
}